kernel.renderer.registrar_pagelet("zona",function(a){var b="#"+a.id;return{onload:function(){sessionStorage.hay_cambios=false;$(b+" .js-combo-nav").on("change",function(){var c=$(this).find(":selected").data("link");if(sessionStorage.hay_cambios=="true"){kernel.ui.prompt({msg:a.mensajes.msg_prompt_perdera_cambios,msg_ok:a.mensajes.aceptar,msg_cancel:a.mensajes.cancelar,ok:function(){guarani.cambiar_op({href:c})}})}else{guarani.cambiar_op({href:c})}});$("#js-colapsar-detalles-zona").on("click",function(){$("#zona-detalle").slideToggle("slow")})}}},"_comun/zonas");